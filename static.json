{
  "root": ".",
  "clean_urls": false,
  "https_only": true,
  "headers": {
    "/dist/**": {
      "Cache-Control": "public, max-age=31536000, immutable",
      "Access-Control-Allow-Origin": "*"
    },
    "/dist/output.css": {
      "Content-Type": "text/css"
    },
    "/dist/*.js": {
      "Content-Type": "application/javascript"
    },
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    }
  },
  "error_page": "index.html"
} 